/* TOOLBAR */

#toolbar-container {
	 background-color: yellow;
     background-repeat: repeat;
     height: 40px;
     margin-left: 1%;margin-right: 1%;
    border: 2px solid rgba(0,0,0,0.2);
    padding: 5px;
}


#toolbar-container img {
    width: 50px;
    height: 50px;
    margin: 1px;
    border: 3px solid rgba(0,0,0,0.1);
    color: #e8ebf0;
    font-weight: bold;
    font-size: 12px;
    cursor: pointer;
    display: block;
    float: left;
    outline: none;
}

#toolbar-container button {
    width: 40px;
    height: 38px;
   
   
    font-weight: bold;
    font-size: 7px;
    cursor: pointer;
    display: block;
    float: left;
    outline: none;
}

.image-upload > input
{
    display: none;
}
.palette-upload > input
{
    display: none;
}
#toolbar-container button:hover {
    -moz-box-shadow:    inset 0 0 25px rgba(255,255,255,0.2)
    -webkit-box-shadow: inset 0 0 25px rgba(255,255,255,0.2);
    box-shadow:         inset 0 0 25px rgba(255,255,255,0.2);
}

#toolbar-container button:active {
    -moz-box-shadow:    inset 0 0 25px rgba(0,0,0,0.5)
    -webkit-box-shadow: inset 0 0 25px rgba(0,0,0,0.5);
    box-shadow:         inset 0 0 25px rgba(0,0,0,0.5);
}

#toolbar-container img {
    width: 30px;
    height: 30px;
}
#fileName {
	
    margin: 1px 1px 1px 1px;
    border: 3px solid rgba(0,0,0,0.1);
    height: 35px;
    font-weight: bold;
    font-size: 12px;
    cursor: pointer;
    display: block;
    float: left;
    outline: none;
}
iframe#some-dialog {
    width: 100% !important;
    box-sizing: border-box;
    -moz-box-sizing: border-box;
}
